theory ExtraInv8_VC362
  imports ExtraInv8
begin

theorem "VC362 extraInv8 env s0 PdOut_value paid_value opened_value"
  apply(unfold VC362_def extraInv8_def)
  apply rule+
   apply simp
  apply(erule conjE)
  subgoal premises vc_prems
    apply(insert vc_prems(1))
    apply(erule conjE)+
    subgoal premises ei
      apply(insert vc_prems(2)[simplified] ei(2))
      apply rule
      using ei(3) apply simp
      apply rule
      using ei(4) apply simp
      apply rule
      using ei(5) substate_refl  apply simp
      apply rule
      using ei(6) apply simp
      apply rule
      using ei(7) apply simp
      apply rule
      using ei(8) apply simp
      apply rule
      using ei(9) apply simp
      apply rule
      using ei(10) apply simp
      apply rule
       apply(insert ei(11))[1]
       apply((erule pattern7_simp); (auto simp add: substate_refl toEnvNum_id))
      apply rule
       apply simp
      apply rule
       apply simp
      apply rule
       apply simp
      by simp
    done
  done

theorem "VC410 extraInv8 env s0 PdOut_value paid_value opened_value"
  apply(unfold VC410_def)
  by simp

end


